{
  "name": "wakmusic-pc",
  "version": "0.1.0",
  "description": "Waktaverse Music Web / Desktop App",
  "repository": "https://github.com/wakmusic/wakmusic-pc.git",
  "author": "wakmusic",
  "private": true,
  "main": "src/electron.js",
  "scripts": {
    "dev": "concurrently -k \"yarn dev:web\" \"tsc src/electron.ts && electron .\"",
    "dev:web": "vite --port 3000",
    "dev:electron": "tsc src/electron.ts && BUILD_TYPE=1 electron .",
    "build:web": "vite build",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "eslint src --ext .ts,.tsx --fix && prettier --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@tanstack/react-virtual": "beta",
    "axios": "^1.4.0",
    "electron-is-dev": "^2.0.0",
    "firebase": "^9.23.0",
    "framer-motion": "^10.13.1",
    "lodash.throttle": "^4.1.1",
    "lottie-react": "^2.4.0",
    "overlayscrollbars": "^2.2.1",
    "overlayscrollbars-react": "^0.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.0",
    "react-is": "^18.2.0",
    "react-router-dom": "^6.13.0",
    "recoil": "^0.7.7",
    "styled-components": "^6.0.0-rc.3"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "babel-plugin-styled-components": "^2.1.4",
    "concurrently": "^8.2.0",
    "electron": "^25.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.8",
    "typescript": "5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "packageManager": "yarn@3.6.0"
}
